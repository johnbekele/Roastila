{
  "expo": {
    "name": "Roastila",
    "slug": "Roastila",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "roastila",
    "userInterfaceStyle": "automatic",
    "platforms": ["ios", "android", "web"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yohansbekele.roastila"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.yohansbekele.roastila"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "reactCompiler": true
    },
    "splash": {
      "backgroundColor": "#ffffff",
      "resizeMode": "contain"
    }
  }
}
